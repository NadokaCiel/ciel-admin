{
  "name": "ciel-admin",
  "version": "0.3.0",
  "private": true,
  "scripts": {
    "lint": "vue-cli-service lint",
    "husky": "node node_modules/husky/lib/installer/bin install",
    "unhusky": "node node_modules/husky/lib/installer/bin uninstall",
    "commit": "ziu commit",
    "img": "ziu img src",
    "cl": "ziu cl -p angular -i CHANGELOG.md -s -r 1000",
    "postversion": "ziu cl -p angular -i CHANGELOG.md -s -r 2 && git add CHANGELOG.md package.json && git add -u",
    "serve": "npm run dev",
    "dev": "npm run build -- development",
    "build": "node scripts/bin/build.js",
    "staging": "npm run build -- staging",
    "testing": "npm run build -- testing",
    "report": "npm run build -- production -r"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.1",
    "animate.css": "^3.7.2",
    "ciel-localstorage": "^1.1.5",
    "cnfapi": "^2.1.3",
    "core-js": "^2.6.12",
    "crypto-js": "^3.3.0",
    "element-ui": "^2.14.1",
    "flex.css": "^1.1.7",
    "konva": "^7.2.1",
    "lodash": "^4.17.20",
    "mksign": "^2.0.1",
    "normalize.css": "^8.0.1",
    "popmotion": "^8.7.6",
    "sortablejs": "^1.12.0",
    "vue": "^2.6.12",
    "vue-cookie": "^1.1.4",
    "vue-konva": "^2.1.6",
    "vue-router": "^3.4.9",
    "vue2-editor": "^2.10.2",
    "vuedraggable": "^2.24.3",
    "vuex": "^3.6.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.12.1",
    "@vue/cli-plugin-eslint": "^3.12.1",
    "@vue/cli-service": "^3.12.1",
    "@vue/eslint-config-airbnb": "^4.0.1",
    "babel-eslint": "^10.1.0",
    "child_process": "^1.0.2",
    "commitlint": "^8.3.5",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.3",
    "husky": "^2.7.0",
    "minimist": "^1.2.5",
    "node-sass": "^4.14.1",
    "sass-loader": "^7.3.1",
    "vue-cli-plugin-ziu": "^1.0.3",
    "vue-template-compiler": "^2.6.12",
    "ziu": "^1.6.4"
  }
}
