{
  "name": "ciel-admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "vue-cli-service lint",
    "husky": "node node_modules/husky/lib/installer/bin install",
    "unhusky": "node node_modules/husky/lib/installer/bin uninstall",
    "commit": "ziu commit",
    "img": "ziu img src",
    "cl": "ziu cl -p angular -i CHANGELOG.md -s -r 1000",
    "postversion": "ziu cl -p angular -i CHANGELOG.md -s -r 2 && git add CHANGELOG.md package.json && git add -u",
    "serve": "npm run dev",
    "dev": "npm run build -- development",
    "build": "node scripts/bin/build.js",
    "staging": "npm run build -- staging",
    "testing": "npm run build -- testing",
    "report": "npm run build -- production -r"
  },
  "dependencies": {
    "animate.css": "^3.7.2",
    "ciel-localstorage": "^1.1.3",
    "cnfapi": "^2.0.2",
    "core-js": "^2.6.5",
    "element-ui": "^2.12.0",
    "flex.css": "^1.1.7",
    "font-awesome.css": "^4.7.2",
    "lodash": "^4.17.15",
    "mksign": "^2.0.1",
    "normalize.css": "^8.0.1",
    "vue": "^2.6.10",
    "vue-cookie": "^1.1.4",
    "vue-router": "^3.0.3",
    "vue2-editor": "^2.10.2",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "child_process": "^1.0.2",
    "commitlint": "^8.1.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "husky": "^2.4.1",
    "minimist": "^1.2.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.1.0",
    "vue-cli-plugin-ziu": "^1.0.2",
    "vue-template-compiler": "^2.6.10",
    "ziu": "^1.6.2"
  }
}
